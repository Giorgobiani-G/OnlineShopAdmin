@model IEnumerable<YearAndMonthViewModel>
@{
    ViewData["Title"] = "YearAndMonth";
}





<h1 class="font-weight-bold" >ByYear</h1>
<table class="table table-striped table-hover">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th>
            @Html.DisplayNameFor(model => model.Sum)
            </th>
        </tr>
    </thead>

    <tbody>
       @foreach (var item in Model.Last().Dictionary)
        {
            <tr>

                <td>
                    @item.Key
                </td>
                <td>
                    @item.Value
                </td>
       
            </tr>

        }
    </tbody>


</table>

<br />

<h1 class="font-weight-bold">ByMonth</h1>


<table class="table table-striped table-hover">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Month)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sum)



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @item.Year
                </td>
                <td>
                    @item.Month
                </td>
                <td>
                    @item.Sum
                </td>

            </tr>

        }
    </tbody>


</table>




