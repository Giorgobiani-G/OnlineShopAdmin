@model IEnumerable<OnlineShopAdmin.DataAccess.ViewModels.ProductsReportViewModel>
@{
    ViewData["Title"] = "ProductCateGories";
}

<h1 class="font-weight-bold" >ProductCateGories</h1>

<br />

<table class="table table-striped table-hover">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
            @Html.DisplayNameFor(model => model.Sum)
            </th>
        </tr>
    </thead>

    <tbody>
       @foreach (var item in Model)
        {
            <tr>

                <td>
                    @item.Name
                </td>
                <td>
                    @item.Sum
                </td>
       
            </tr>
        }
    </tbody>


</table>
